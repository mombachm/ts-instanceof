{
  "defaultSeverity": "error",
  "extends": [
      "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "no-namespace": false,
    "trailing-comma": false,
    "object-literal-sort-keys": false,
    "no-empty": false,
    "ordered-imports": false,
    "member-ordering": false,
    "no-console": false,
    "no-string-literal": false,
    "max-classes-per-file": false,
    "object-literal-key-quotes": false,
    "variable-name": false,
    "whitespace": [
      true,
      "check-branch",
      "check-decl",
      "check-operator",
      "check-module",
      "check-separator",
      "check-rest-spread",
      "check-type",
      "check-typecast",
      "check-type-operator",
      "check-preblock"
    ],
    "interface-name": [true, "never-prefix"]
  }
}
